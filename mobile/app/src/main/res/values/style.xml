<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:tools="http://schemas.android.com/tools">
    <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <item name="colorPrimary">@color/material_dynamic_primary0</item>
    </style>
    <style name="DialogFilter" parent="Theme.AppCompat.Light.DialogWhenLarge">
        <item name="android:statusBarColor" tools:targetApi="lollipop">#95D1CC</item>
        <item name="windowNoTitle">true</item>
        <item name="android:windowAnimationStyle">@style/Theme.AppCompat.Dialog</item>
    </style>
    <style name="CustomRatingBar" parent="@android:style/Widget.RatingBar">
        <item name="android:progressDrawable">@drawable/favorite_rating_bar_full</item>
        <item name="android:minHeight">30dp</item>
        <item name="android:maxHeight">30dp</item>
    </style>
    <style name="roundedImageViewRounded">
        <item name="cornerFamily">rounded</item>
        <item name="cornerSize">50%</item>
    </style>

</resources>